val term : type.
val nat : type.
val s : nat -> nat.
val z : nat.
val plus : nat -> nat -> nat.
val double : nat -> nat.
assert forall (X:nat). ((plus z X) = X).
assert forall (X:nat). (forall (Y:nat). ((plus (s X) Y) = (s (plus X Y)))).
assert forall (X:nat). ((double (s X)) = (s (s (double X)))).
assert (double z) = z.
goal forall (X:nat). ((double X) = (plus X X)).
