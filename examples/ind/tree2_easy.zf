val term : type.
val tree : type.
val empty : tree.
val node : tree -> term -> tree -> tree.
val rev : tree -> tree.
assert (rev empty) = empty.
assert
  forall (L:tree).
    (forall (R:tree).
       (forall (X:term). ((rev (node L X R)) = (node (rev R) X (rev L))))).
goal forall (T:tree). ((rev (rev T)) = T).
