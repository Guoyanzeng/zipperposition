include "tree.zf".
val p : tree -> prop.
val q : tree -> prop.
assert p empty.
assert q empty.
assert
  forall (L:tree).
    (forall (X:term).
       (forall (R:tree).
          (((q R) && (q L) && (p L) && (p R)) => (q (node L X R))))).
assert
  forall (L:tree).
    (forall (X:term).
       (forall (R:tree).
          (((q R) && (q L) && (p L) && (p R)) => (p (node L X R))))).
goal forall (T:tree). (p T).
