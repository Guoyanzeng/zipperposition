tff(declplus, type, '+' : ($i*$i)>$i, attrs(ac, infix('+'))).
fof(seq_0, axiom, ![N_1]: '+'('0', N_1) = N_1).
fof(seq_1, axiom, ![N_1,N_0]: '+'(N_1, N_0) = '+'(N_0, N_1)).
fof(seq_2, axiom, ![N_1,N_0]: '+'(N_1, '+'(N_0, '1')) = '+'('+'(N_1, N_0), '1')).
fof(seq_3, axiom, ![X,X_0,X_1]: (f(X) != X_0 | ~ '<'(s_7('q_{1}'(X_0)), '+'(s_7('q_{1}'(X_0)), '1')) | ~ '<'(s_8('q_{1}'(X_0), X_0, s_7('q_{1}'(X_0))), X_1) | f(X_1) != X_0)).
fof(seq_4, axiom, ![X,X_0,X_1,N_2]: (f(X) != X_0 | ~ '<'(s_8('q_{1}'(X_0), X_0, s_7('q_{1}'(X_0))), X_1) | f(X_1) != X_0 | '<'(N_2, '+'(N_2, '1')))).
fof(seq_5, axiom, ![N_0,N_1]: (f('+'('+'(N_0, N_1), '1')) = '0' | f('+'('+'(N_0, N_1), '1')) = '1')).
fof(seq_6, axiom, ![N_1,N_0]: '<'('+'('0', N_1), '+'('+'(N_0, '1'), N_1))).
