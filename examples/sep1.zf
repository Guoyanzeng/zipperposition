
# expect: unsat

# separation logic
# SWW413-1.p

include "sep_ax.zf".

val x1:list.
val x2:list.
val x3:list.
val x4:list.

assert x1 != nil.
assert x2 != nil.

assert x1 != x2.
assert x2 != x3.
